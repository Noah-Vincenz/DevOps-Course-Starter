# Flask server configuration.
FLASK_APP=test_app
FLASK_ENV=test

# Change the following values for local development.
SECRET_KEY=secret-key

# To disabled oauth in tests
LOGIN_DISABLED=True

# MONGO DB
TODO_LIST_ID=todo_list_id
DOING_LIST_ID=doing_list_id
